  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "mkdirp": "^2.1.5",
    "ncp": "^2.0.0",
    "vite": "^4.2.0"
  },
  "dependencies": {
    "@arcgis/core": "^4.26.5",
    "@esri/calcite-components": "1.0.7"
  }
}
